{
  "name": "skywalk",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "buildanddeploy:dev": "firebase use development && ng build --prod --env=dev && firebase deploy",
    "buildanddeploy:prod": "firebase use production && ng build --prod && firebase deploy",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr": "node functions/dist/server",
    "clean:hosting": "rm -rf ./dist && rm -rf ./public && rm -rf ./functions/dist",
    "copy:hosting": "cp -r ./dist/browser/. ./public && rm ./public/index.html",
    "build:client-and-server-bundles:dev": "./node_modules/@angular/cli/bin/ng build && npm run copy:hosting && ./node_modules/@angular/cli/bin/ng run skywalk:server",
    "build:client-and-server-bundles:prod": "./node_modules/@angular/cli/bin/ng build -c production --prod && npm run copy:hosting && ./node_modules/@angular/cli/bin/ng run skywalk:server:production",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr:dev": "npm run clean:hosting && npm run build:client-and-server-bundles:dev && npm run webpack:server",
    "build:ssr:prod": "npm run clean:hosting && npm run build:client-and-server-bundles:prod && npm run webpack:server",
    "build:functions": "npm run --prefix functions build",
    "build:dev": "npm run build:ssr:dev && npm run build:functions",
    "build:prod": "npm run build:ssr:prod && npm run build:functions"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.3",
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "5.2.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/fire": "^5.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "5.2.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ks89/angular-modal-gallery": "6.2.0",
    "@ngu/carousel": "1.4.8",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "2.0.1",
    "@types/hammerjs": "2.0.35",
    "@types/mousetrap": "1.6.0",
    "bootstrap": "4.0.0-beta.2",
    "core-js": "2.4.1",
    "firebase": "^5.5.0",
    "hammerjs": "2.0.8",
    "mousetrap": "1.6.2",
    "ng2-ckeditor": "1.2.1",
    "ngx-wow": "1.0.2",
    "rxjs": "6.3.2",
    "rxjs-compat": "6.3.2",
    "wowjs": "1.1.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "5.0.0",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "4.0.1",
    "express": "^4.15.2",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "pre-commit": "^1.2.2",
    "protractor": "~5.1.2",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^4.2.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.1.0"
  }
}
